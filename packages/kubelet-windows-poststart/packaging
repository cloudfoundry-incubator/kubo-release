. ./exiter.ps1

$ErrorActionPreference = "Stop";
trap { $host.SetShouldExit(1) }

. C:\var\vcap\packages\golang-1.10-windows\bosh\compile.ps1

mkdir ../src
copy-item -recurse * ../src/
mv ../src/ ./src

mkdir $env:BOSH_INSTALL_TARGET/bin

go build -o $env:BOSH_INSTALL_TARGET/bin/poststart.exe src/kubelet-windows-poststart/main.go
