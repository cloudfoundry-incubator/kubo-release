kind: KubeletConfiguration
apiVersion: kubelet.config.k8s.io/v1beta1
authentication:
  anonymous:
    enabled: false
  webhook:
    enabled: true
authorization:
  mode: Webhook
clusterDNS: 
  - <%= kubedns-service-ip %>
clusterDomain: cluster.local
failSwapOn: false
readOnlyPort: 0
serializeImagePulls: false
