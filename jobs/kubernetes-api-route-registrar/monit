checn process kubernetes-api-route-registrar
  with pidfile /var/vcap/sys/run/kubernetes/kubernetes-api-route-registrar.pid
  start program "/var/vcap/jobs/kubernetes-api-route-registrar/bin/kubernetes-api-route-registrar_ctl start"
  stop program "/var/vcap/jobs/kubernetes-api-route-registrar/bin/kubernetes-api-route-registrar_ctl stop"
  group vcap
