check process kubernetes-api-route-registrar
  with pidfile /var/vcap/sys/run/kubernetes-api-route-registrar/kubernetes-api-route-registrar.pid
  start program "/var/vcap/jobs/kubernetes-api-route-registrar/bin/kubernetes-api-route-registrar-ctl start"
  stop program "/var/vcap/jobs/kubernetes-api-route-registrar/bin/kubernetes-api-route-registrar-ctl stop"
  group vcap
