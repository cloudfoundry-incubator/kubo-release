apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kube-scheduler/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kube-scheduler
  name: kube-scheduler
current-context: kube-scheduler
users:
- name: kube-scheduler
  user:
    token: "<%= p("api-token") %>"
