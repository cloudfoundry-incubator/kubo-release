<% if_p("kube-proxy-password") do %>
apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubeconfig/config/ca.pem"
    server: https://master.kubo:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kube-proxy
  name: kube-proxy
current-context: kube-proxy
users:
- name: kube-proxy
  user:
    token: "<%= p("kube-proxy-password") %>"
<% end %>
