<% if_p("kube-scheduler-password") do %>
apiVersion: v1
kind: Config
clusters:
- cluster:
    server: http://localhost:8080
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kube-scheduler
  name: kube-scheduler
current-context: kube-scheduler
users:
- name: kube-scheduler
  user:
    token: "<%= p("kube-scheduler-password") %>"
<% end %>
