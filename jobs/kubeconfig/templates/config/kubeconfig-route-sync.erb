<% if_p("route-sync-password") do %>
apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubeconfig/config/ca.pem"
    server: https://master.kubo:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: route-sync
  name: route-sync
current-context: route-sync
users:
- name: route-sync
  user:
    token: "<%= p("route-sync-password") %>"
<% end %>
