apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubeconfig/config/ca.pem"
    server: <%= p("kubernetes-api-url") %>
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet
  name: kubelet
current-context: kubelet
users:
- name: kubelet
  user:
    token: "<%= p("kubelet-password") %>"
