apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubernetes-system-specs/config/ca.pem"
    server: https://master.kubo:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet
  name: kubelet
current-context: kubelet
users:
- name: kubelet
  user:
    token: "<%= p("kubelet-password") %>"
